{
  "name": "rdview-service",
  "version": "1.1.0",
  "description": "Rdview service for loading road photos",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "tsc -w && rollup -c",
    "test": "jest",
    "test:watch": "ject --watch"
  },
  "keywords": [
    "rdview",
    "road",
    "photo"
  ],
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/jest": "^23.3.1",
    "jasmine": "^3.1.0",
    "jasmine-core": "^3.1.0",
    "jest": "^23.4.2",
    "rollup": "^0.63.5",
    "rollup-plugin-commonjs": "^9.1.4",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
