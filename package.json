{
  "name": "rdview-service",
  "version": "1.0.0",
  "description": "Rdview service for loading road photos",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "tsc -w && rollup -c",
    "test": "jest",
    "test:watch": "ject --watch"
  },
  "keywords": [
    "rdview",
    "road",
    "photo"
  ],
  "author": "",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "@types/jest": "^21.1.8",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "jest": "^21.2.1",
    "rollup": "^0.52.1",
    "rollup-plugin-commonjs": "^8.2.6",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "axios": "^0.17.1"
  }
}
